<h1 style="text-align:center">CURRENTLY REGISTERED USERS</h1>

<table id="userlist" style="text-align:center">
  <tr>
    <th>Username</th>
    <th>Link to blog</th> 
  </tr>

<% @users.each do |user| %>

  <tr>
    <td><%= user.username %></td>
    <td> <%= link_to "view", user_path(user.username)%></td>
  </tr>
<% end %>
</table>